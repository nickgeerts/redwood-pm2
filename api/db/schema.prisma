datasource DS {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Framework {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  claps     Int      @default(0)
  createdAt DateTime @default(now())
}
